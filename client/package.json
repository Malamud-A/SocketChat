    "style-loader": "^0.23.1",
    "url-loader": "1.1.2",
    "webpack": "^4.34.0",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "4.1.4"
  }
